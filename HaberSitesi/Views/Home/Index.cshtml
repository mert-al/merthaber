                                foreach (Video item in videos)
                                {
                                    <div class="single-video mr-2">
                                        <div class="video-items-active">
                                            <video controls id="videoPlayer" class="video-js vjs-default-skin" width="500" height="300" data-setup='{ "aspectRatio":"1920:1080", "playbackRates": [0.5,1, 1.5, 2, 2.5 , 3]  }'>
                                                <source src="@ViewBag.apiUrl/@item.EmbedUrl" type="video/mp4">
                                            </video>
                                        </div>
                                        <div class="video-intro">
                                            <h4>@item.Title</h4>
                                        </div>
                                    </div>
                                }
                            }
                            <button type="button" class="slick-prev slick-arrow mt-3"><span class="ti-arrow-left"><!--<span--></span></button>
                            <button type="button" class="slick-next slick-arrow mt-3"><span class="ti-arrow-right"></span></button>

                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>  <!-- End Start youtube -->
    <!--  Son Haberler start -->
    <div class="recent-articles">
        <div class="container">
            <div class="recent-wrapper">
                <!-- section Tittle -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="section-tittle mb-30">
                            <h3>Son Haberler</h3>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="recent-active dot-style d-flex dot-style">

                            @{
                                foreach (NewsAlbumMix item in newsAlbumMixes.GetRange(0, 5))
                                {
                                    <div class="single-recent mb-100">
                                        <div class="what-img">

                                            <a href="~/@item.path@item.Categories.FirstOrDefault().url/@item.url">
                                                <img src="@ViewBag.apiUrl/@item.OtherIMG" alt="">
                                            </a>
                                        </div>
                                        <div class="what-cap">
                                            <span class="color1">@item.Categories.FirstOrDefault().Name</span>
                                            <h4>
                                                <a href="~/@item.path@item.Categories.FirstOrDefault().url/@item.url">@item.Title</a>
                                            </h4>
                                        </div>
                                    </div>
                                }
                                newsAlbumMixes.RemoveRange(0, 5);
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</main>
